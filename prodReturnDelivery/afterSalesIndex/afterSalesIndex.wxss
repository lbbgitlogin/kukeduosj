page {
  width: 100%;
  height: 100%;
  position: relative;
}

.title-nav {
  width: 100%;
  height: 84rpx;
  background: #fff;
  font-size: 26rpx;
  line-height: 84rpx;
  color: #9d9d9d;
  /* font-weight: bold; */
  display: flex;
  align-items: center;
  justify-content: space-around;
  /* border-bottom: 2rpx solid #efefef; *//* box-shadow: 0 -2rpx 0 0 #efefef, 0 2rpx 0 0 #efefef; */
}

.title-nav .items {
  height: 100%;
  box-sizing: border-box;
  color: #db384c;
  border-bottom: 4rpx solid #db384c;
}

.content-box {
  width: 100%;
  background: #fff;
  /* box-shadow: 0 -2rpx 0 0 #efefef, 0 2rpx 0 0 #efefef; */
  margin-bottom: 20rpx;
  border-top: 2rpx solid #efefef;
  border-bottom: 2rpx solid #efefef;
  box-sizing: border-box;
}

.panel-hd {
  padding: 32rpx 40rpx 31rpx 23rpx;
  color: #999;
  font-size: 24rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #efefef;
  /* box-shadow: 0 -2rpx 0 0 #efefef, 0 2rpx 0 0 #efefef; */
}

.panel-hd label {
  float: right;
  color: #db384c;
}

.product {
  margin-left: 22rpx;
  width: 728rpx;
  box-sizing: border-box;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-right: 58rpx;
  padding-left: 22rpx;
  display: flex;
  /* align-items: center; *//* box-shadow: 0 2rpx 0 0 #efefef; *//* border-top:2rpx solid #efefef; */
  border-bottom: 2rpx solid #efefef;
}

.product-img image {
  width: 144rpx;
  height: 144rpx;
}

.product-info {
  margin-left: 10rpx;
  width: 100%;
  position: relative;
}

.product-name {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 7rpx;
  color: #000;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.product-specification {
  font-size: 24rpx;
  color: #9b9b9b;
  margin-bottom: 24rpx;
}

.product-num {
  width: 95%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  color: #9b9b9b;
  position: absolute;
  left: 0rpx;
  bottom: 0;
}

.product-price {
  font-size: 28rpx;
  font-weight: bold;
  color: #e20505;
}

.m-total-info {
  text-align: right;
  padding: 10rpx 40rpx;
  font-size: 24rpx;
  color: #9b9b9b;
  box-sizing: border-box;
}

.m-total-info label {
  font-size: 32rpx;
  color: #db384c;
}

.total {
  font-size: 24rpx;
  font-weight: bold;
  color: #030303;
  margin-bottom: 12rpx;
}

.link-btn-box {
  width: 100%;
  height: 96rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding-left: 23rpx;
  padding-right: 40rpx;
  font-size: 24rpx;
  color: #9b9b9b;
  /* box-shadow: 0 -2rpx 0 0 #efefef, 0 2rpx 0 0 #efefef; */
  border-top: 2rpx solid #efefef;
}

.link-btn {
  height: 95rpx;
  padding-top: 23rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
  text-align: right;
  /* box-shadow: 0 -2rpx 0 0 #efefef, 0 2rpx 0 0 #efefef; */
  border-top: 2rpx solid #efefef;
}

.total-info {
  display: flex;
  align-items: center;
  height: 128rpx;
  padding: 10rpx 15px;
  font-size: 24rpx;
  color: #9b9b9b;
  box-sizing: border-box;
  background: #fcfcfc;
}

.condition {
  /* width: 170rpx; */
  width: 200rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: bold;
  color: #000;
  display: flex;
  align-items: center;
  justify-content: center;
  /* margin-right: 24rpx; */
}

.t-text {
  padding-left: 30rpx;
  box-sizing: border-box;
  border-left: 2rpx solid #efefef;
}

.link-btn .u-link-btn:not(:last-child) {
  margin-right: 20rpx;
}

.link-for {
  border: 1px solid #999;
  color: #999;
}

.nullpage {
  width: 100%;
  height: 80rpx;
  text-align: center;
  position: fixed;
  left: 0;
  top: 50%;
  margin-top: -40rpx;
}

.site-bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 4;
}

.site-box {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 5;
  width: 100%;
  background: #fff;
  padding-bottom: 35rpx;
  box-sizing: border-box;
}

.s-title {
  width: 100%;
  height: 96rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: #000;
  padding: 0 23rpx;
  box-sizing: border-box;
  box-shadow: 0 -2px 0 0 #efefef, 0 2px 0 0 #efefef;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.site {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  font-size: 24rpx;
  color: #000;
  margin-top: 28rpx;
  padding: 0 23rpx;
  box-sizing: border-box;
}

.site input {
  margin-left: 24rpx;
}

.link-service {
  width: 560rpx;
  height: 260rpx;
  position: fixed;
  left: 50%;
  margin-left: -280rpx;
  top: 50%;
  margin-top: -200rpx;
  /* box-sizing: border-box; */
  padding-top: 30rpx;
  z-index: 10;
  overflow: hidden;
  background: #fff;
  border-radius: 8px;
}

.l-text {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #888;
  text-align: center;
}

.l-btn-box {
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  bottom: 0;
}

.l-btn-box > view {
  width: 302rpx;
  height: 90rpx;
  text-align: center;
  box-sizing: border-box;
  font-size: 36rpx;
  line-height: 90rpx;
  color: #000;
  border-top: 2rpx solid #e5e5e5;
}

.l-link {
  position: relative;
  border-left: 2rpx solid #e5e5e5;
}

.l-link contact-button {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0;
}

.end {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  text-align: center;
  font-size: 20rpx;
  line-height: 24rpx;
  background: #e9e9e9;
  border-radius: 50%;
  vertical-align: middle;
  margin-left: 4rpx;
}

.navtext1 contact-button, .navtext contact-button {
  width: 100%;
  height: 100%;
  position: absolute;
  right: 0;
  top: 0;
  opacity: 0;
  text-align: right;
}
