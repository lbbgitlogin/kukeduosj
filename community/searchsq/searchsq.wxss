.m-cart-del{line-height:70rpx;font-size: 32rpx;}
/*底部按钮 end*/
.del{-webkit-animation: mdel .2s  1;animation: mdel .2s  1;left: -100rpx;}
@-webkit-keyframes mdel {
  0% {left: 0;}
  100% {left: -100rpx;}
}
@keyframes mdel {
  0% {left: 0;}
  100% {left: -100rpx}
}
