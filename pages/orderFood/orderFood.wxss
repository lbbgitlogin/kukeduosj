@import '../../tpl/indexTpl/indexTpl.wxss';

.header {
  width: 100%;
  height: 90rpx;
  background: rgb(251, 229, 177);
  text-align: center;
  font-size: 28rpx;
  line-height: 90rpx;
}

.have {
  color: #db384c;
}

.m-cell-ft::after {
  content: " ";
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #db384c;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: relative;
  top: -4rpx;
  margin-top: -8rpx;
  right: -185rpx;
}
