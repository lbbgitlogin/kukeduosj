.m-navbar-item {padding: 20rpx 0;font-size: 24rpx;}
.m-navbar-item:after {border: none;}
.m-navbar-item.m-navbar-item-on {background-color: #fff;color: #DB384C;}
.m-navbar-item.m-navbar-item-on::before {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 6rpx;border-bottom: 6rpx solid #DB384C;color: #CCCCCC;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);z-index: 3;}
.m-panel-hd label{float: right;color: #DB384C;}
.m-total-info{text-align: right;padding: 20rpx 15px;font-size: 24rpx;}
.m-total-info label{font-size: 32rpx;color: #DB384C;}
.m-total-btn{text-align: right;padding: 20rpx 20rpx 30rpx 20rpx;position: relative;}
.m-total-btn::before {content:" ";position: absolute;left: 0;top: 0;right: 0;height: 1px;border-top: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.m-total-btn .u-link-btn{margin: 0 10rpx;}
.m-sort{position: relative}
.m-sort image{width: 24rpx; height: 24rpx;position: absolute;top: 50%;margin-top: -12rpx;margin-left:2rpx;}
.m-product-price text{font-size: 28rpx;color: #888;}
.m-tab{position: fixed;width: 100%;top: 0;z-index: 2;}
.m-orderlist{padding-top:70rpx;position: absolute;box-sizing: border-box;width: 100%;height: 100%; }
.progressBarBox{position: absolute;right: 0;bottom: 38%;color: #888;font-size: 24rpx;margin-right: 20rpx;}
.u-progressBar{display:inline-block;width: 140rpx;height: 20rpx;background-color: #d5d5d5;border-radius: 20rpx;overflow: hidden;}
.u-progressBar-cont{height: 20rpx;background-color: #ffc001;}
.progress-bar {
  width: 500rpx;
  height: 26rpx;
  border: 5rpx solid rgb(89, 10, 35);
  background: rgb(89, 10, 35);
  border-radius: 40rpx;
  position: relative;
}

.left {
  border-radius: 40rpx;
  background: rgb(255, 231, 0);
  height: 100%;
  float: left;
}
.u-back2 image{border-radius: 100%;width: 77rpx;height: 77rpx;border: 1px solid #eee;font-size: 20rpx;text-align: center;background-color: #fff;box-shadow: 0px 4rpx 8rpx rgba(0,0,0,0.35);z-index: 999;opacity: 0.8;line-height:77rpx; margin-bottom: 20rpx;}

.m-product-price .u-btn{padding:0;margin: 0 20rpx;width: 160rpx;float: right;font-size: 24rpx;line-height: 50rpx;}
/* margin-top: 10rpx; */
.groupNumber{position: absolute;bottom: 6rpx;left: 0;font-size:24rpx;color: #db384c;background: #fbebeb;text-align: center;padding: 0 10rpx}

.u-del-price{color:#888888;font-size: 20rpx; text-decoration:line-through;}