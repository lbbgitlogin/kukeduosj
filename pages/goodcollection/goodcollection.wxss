.m-product-item{position: relative;overflow: hidden}
.u-del{width: 150rpx;background-color: #DB384C;float: right;height: 100%;color: #fff;font-size: 28rpx;text-align: center;line-height: 212rpx;}
.m-item-c{position: absolute;height: 100%;width: 902rpx;}
.del{-webkit-animation: mdel .2s  1;animation: mdel .2s  1;left: -150rpx;}
.m-addCart{float: right;color: #888;font-size: 38rpx !important;margin-right: 40rpx;}
.m-product-info{height: 211rpx;width:538rpx;float: left;box-sizing: border-box;padding:20rpx 0;position: relative;}
@-webkit-keyframes mdel {
  0% {left: 0;}
  100% {left: -150rpx;}
}
@keyframes mdel {
  0% {left: 0;}
  100% {left: -150rpx}
}
.u-pa .m-product-list {padding: 0;}
.u-back2 image{border-radius: 100%;width: 77rpx;height: 77rpx;border: 1px solid #eee;font-size: 20rpx;text-align: center;background-color: #fff;box-shadow: 0px 4rpx 8rpx rgba(0,0,0,0.35);z-index: 999;opacity: 0.8;line-height:77rpx; margin-bottom: 20rpx;}