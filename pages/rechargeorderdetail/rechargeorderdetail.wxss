.paysuccess{
  height: 380rpx;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  color: #000;

}
.content{
  height: 180rpx;
  background: #fff;
  padding: 0 30rpx
}
.store{
  font-size: 28rpx;
  color: #000;
  height: 80rpx;
  border-bottom: 1px solid #d9d9d9;
  line-height: 90rpx;
}
.details{
  display: flex;
  font-size: 24rpx;
  color: #000;
}
.details-left{
  width: 45%;
}
.details-left view{
  margin-top: 40rpx;
}
.details-right{
  width: 55%;
}
.details-right view{
  margin-top: 40rpx;
}
.m-footer-btn{width: 100%;position:fixed;bottom: 0;left: 0;background-color: #fff;text-align: right;border-top: 1rpx solid #d5d5d5;box-sizing: border-box;padding: 20rpx;}
.u-link-btn{margin: 0 10rpx;vertical-align: middle;display: inline-block;line-height: 40rpx;}