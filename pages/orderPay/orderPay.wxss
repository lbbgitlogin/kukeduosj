.totalMoney{
  width: 90%;
  border: 2rpx solid #d9d9d9;
  display: flex;
  margin: 24rpx auto 0;
  justify-content: space-betwee;
  height: 92rpx;
  border-radius: 8rpx;
}
.tishi{
  width: 90%;
  margin: 30rpx auto 0;
  font-size: 24rpx;
  color: #db384c;
  text-align: right
}
.totalMoney label{
  width: 54%;
  height: 92rpx;
  line-height: 92rpx;
  font-size: 26rpx;
  margin-left: 20rpx;
  color: #000;
}
.totalMoney input{
  height: 92rpx;
  line-height: 92rpx;
  font-size: 26rpx;
  text-align: right;
  margin-right: 20rpx;
}
.nodiscount{
  width: 90%;
  margin: 24rpx auto 0;
}
.check{
  height: 58rpx;
  line-height: 58rpx;
  font-size: 26rpx;
  color: #808080;
}
.discount{
  height: 85rpx;
  line-height: 90rpx;
  padding: 0 40rpx;
  background: #fff;
  font-size: 28rpx;
  color: #000;
  margin: 24rpx 0;
}
.pay{
  margin-top: 24rpx;
  padding: 0 40rpx;
  background: #fff;
}
.coupon{
  display: flex;
  justify-content: space-between;
  height: 85rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #000;
}
.payments{
  display: flex;
  justify-content: space-between;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 28rpx;
  color: #000;
}
.payment{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  background: #db384c;
  color: #fff;
  margin-top: 50rpx;
  font-size: 32rpx
}
.explain{
  width: 90%;
  margin: 50rpx auto; 
}
.title{
  font-size: 28rpx;
  color: #000;
}
.content{
  font-size: 24rpx;
  color: #717171;
  margin-top: 10rpx;
}
.m-order-typetxt{color:#DB384C;}