.move1{
position:relative;
width: 125%;
height: 100rpx;
animation:mymove1 4s linear infinite alternate;
-webkit-animation:mymove1 4s linear infinite alternate; /*Safari and Chrome*/}

@keyframes mymove1
{
0%{right:0px}
20%{right:10px;}
40%{right:20px;}
60%{right:30px;}
80%{right:20px;}
to {right:10px;}
}

@-webkit-keyframes mymove1 /*Safari and Chrome*/
{
0%{right:0px}
20%{right:10px;}
40%{right:20px;}
60%{right:30px;}
80%{right:20px;}
to {right:10px;}
}

